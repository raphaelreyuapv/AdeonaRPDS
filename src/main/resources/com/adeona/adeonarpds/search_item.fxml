<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.adeona.adeonarpds.SearchItem">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="urlProfile" fitHeight="100.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img.png" />
               </image></ImageView>
            <VBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="124.0">
               <children>
                  <Label fx:id="title" text="Title">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin></Label>
                  <Label fx:id="location" text="Location">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin></Label>
               </children>
            </VBox>
            <Label fx:id="host" alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="162.0" text="Host" />
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="2.5" left="5.0" right="5.0" top="2.5" />
   </padding>
</AnchorPane>
